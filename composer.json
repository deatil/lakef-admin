{
    "name": "lakef/server-log",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "lakef",
        "server-log",
        "lakef",
        "log"
    ],
    "description": "A server log system base on hyperf, swoole and layui.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "deatil",
            "email": "deatil@github.com",
            "homepage": "https://github.com/deatil",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "hyperf/cache": "~2.1.0",
        "hyperf/command": "~2.1.0",
        "hyperf/config": "~2.1.0",
        "hyperf/db-connection": "~2.1.0",
        "hyperf/framework": "~2.1.0",
        "hyperf/guzzle": "~2.1.0",
        "hyperf/http-server": "~2.1.0",
        "hyperf/logger": "~2.1.0",
        "hyperf/memory": "~2.1.0",
        "hyperf/process": "~2.1.0",
        "hyperf/database": "~2.1.0",
        "hyperf/redis": "~2.1.0",
        "hyperf/constants": "~2.1.0",
        "hyperf/async-queue": "~2.1.0",
        "hyperf/amqp": "~2.1.0",
        "hyperf/model-cache": "~2.1.0",
        "hyperf/elasticsearch": "~2.1.0",
        "hyperf/view": "^2.1.0",
        "hyperf/view-engine": "^2.1.0",
        "hyperf/session": "^2.1.0",
        "hyperf/task": "^2.1.0",
        "hyperf/validation": "^2.1.0",
        "hyperf/tracer": "~2.1.0",
        "hyperf/paginator": "~2.1.0",
        "hyperf/pool": "~2.1.0",
        "hyperf/process": "~2.1.0",
        "hyperf/utils": "~2.1.0",
        "hyperf/filesystem": "~2.1.0",
        "hyperf/rpc-server": "~2.1.0",
        "hyperf/rpc-client": "~2.1.0",
        "hyperf/json-rpc": "~2.1.0",
        "hyperf/kafka": "~2.1.0",
        "donjan-deng/hyperf-permission": "~2.0.0",
        "gregwar/captcha": "1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/devtool": "~2.1.0",
        "hyperf/testing": "~2.1.0",
        "hyperf/watcher": "^2.1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "swoole/ide-helper": "^4.5"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Lakef\\": "module/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": [],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "rm -rf runtime/container"
        ],
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config",
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "php ./bin/hyperf.php start"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
